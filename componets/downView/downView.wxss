/* componets/downView/downView.wxss */
.base{
  display: flex;
  flex-direction: column;
  margin: 30rpx;
  border: 1px solid gray;
  border-radius: 15px;
}

.top{
  margin-top: 30rpx;
}
.date{
  margin-left: 30rpx;
}

.line{
  border: 1px solid gray;
  height: 0px;
  margin: 30rpx;
}

.middle{
  display: flex;
  flex-direction: row;
  margin-left: 30rpx;
  margin-right: 30rpx;
}

.left{
  width: 70%;
  display: flex;
  flex-direction: column;
}

.right{
  width:30%;
  display: flex;
  flex-direction: row;
}

.ring{
  margin-left: auto;
  margin-top:auto;
  margin-bottom: auto;
}

.text{
  margin-top: 15rpx;
  word-break: keep-all;
  word-wrap: break-word;
}

.title{
  font-size: 130%
}


.large-right{
  width:30%;
  display: flex;
  flex-direction: column;
}

.ring-unit{
  display: flex;
  flex-direction: row;
}

.ring-text{
  margin-left: auto;
  margin-top:auto;
  margin-bottom: auto;
  font-size: 120%;
  writing-mode:vertical-rl;
}

.line-h{
  border: 1px solid gray;
  width: 0;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-right: 10rpx;
}

.little{
  margin-left: auto;
  margin-top:auto;
  margin-bottom: auto;
}

.expand-view{
  display: flex;
  flex-direction: row;
  align-items: center
}

.expand-line{
  border: 1px solid gray;
  height: 0px;
  margin-left: 30rpx;
  flex-grow: 1
}

.expand{
  margin-right: 30rpx;
  flex-grow: 0
}

